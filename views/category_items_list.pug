extends layout 

block content 
  p #[strong Category:] 
    a(href=category.url) #{category.name}
  p #[strong Description:] #{category.description}
  if items
    ul 
      each item in items 
        li 
          a(href=item.url)=item.name
  a(href=category.url + '/delete')
    button.btn.btn-primary Delete This Category